<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.UlkeMapper">

    <resultMap type="Ulke" id="UlkeResult">
        <result property="ulkeId"    column="ulke_id"    />
        <result property="name"    column="name"    />
    </resultMap>

    <sql id="selectUlkeVo">
        select ulke_id, name from ulke
    </sql>

    <select id="selectUlkeList" parameterType="Ulke" resultMap="UlkeResult">
        <include refid="selectUlkeVo"/>
        <where>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
        </where>
    </select>

    <select id="selectUlkeByUlkeId" parameterType="Long" resultMap="UlkeResult">
        <include refid="selectUlkeVo"/>
        where ulke_id = #{ulkeId}
    </select>

    <insert id="insertUlke" parameterType="Ulke" useGeneratedKeys="true" keyProperty="ulkeId">
        insert into ulke
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
        </trim>
    </insert>

    <update id="updateUlke" parameterType="Ulke">
        update ulke
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
        </trim>
        where ulke_id = #{ulkeId}
    </update>

    <delete id="deleteUlkeByUlkeId" parameterType="Long">
        delete from ulke where ulke_id = #{ulkeId}
    </delete>

    <delete id="deleteUlkeByUlkeIds" parameterType="String">
        delete from ulke where ulke_id in
        <foreach item="ulkeId" collection="array" open="(" separator="," close=")">
            #{ulkeId}
        </foreach>
    </delete>
</mapper>